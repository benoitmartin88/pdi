cmake_minimum_required(VERSION 3.10)

# Compute the installation prefix relative to this file.
get_filename_component(_PDIcfg_IMPORT_PREFIX "${CMAKE_CURRENT_LIST_FILE}" PATH)
get_filename_component(_PDIcfg_IMPORT_PREFIX "${_PDIcfg_IMPORT_PREFIX}" PATH)
get_filename_component(_PDIcfg_IMPORT_PREFIX "${_PDIcfg_IMPORT_PREFIX}" PATH)
get_filename_component(_PDIcfg_IMPORT_PREFIX "${_PDIcfg_IMPORT_PREFIX}" PATH)
if(_PDIcfg_IMPORT_PREFIX STREQUAL "/")
  set(_PDIcfg_IMPORT_PREFIX "")
endif()

set(PDIcfg_pdicfg_validate_EXECUTABLE "${_PDIcfg_IMPORT_PREFIX}/@CMAKE_INSTALL_BINDIR@/pdicfg_validate" CACHE PATH "path to the pdicfg_validate executable")
add_executable(PDIcfg::pdicfg_validate IMPORTED)
set_target_properties(PDIcfg::pdicfg_validate PROPERTIES IMPORTED_LOCATION "${PDIcfg_pdicfg_validate_EXECUTABLE}")

unset(_PDIcfg_IMPORT_PREFIX)

